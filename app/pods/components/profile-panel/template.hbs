<div class="col-md-8">
	<div class="row">
		<div class="col-md-4 text-center">
			<img class="img-circle avatar avatar-original" style="-webkit-user-select:none;
			display:block; margin:auto; width: 80px; height:80px" src="../user.png">
		</div>
		<div class="col-md-8  text-left">
			<div class="row">
				<div class="col-md-6">
					<h3 class="only-bottom-margin">{{user.fname}} {{user.lname}}</h3>
				</div>
			</div>
			<div class="row">
				<div class="col-md-6">
					<span class="text-muted">Email:{{user.email}}</span> <br>
					<span class="text-muted">User Name:{{user.uname}}</span> <br>
				</div>
			</div>
		</div>
	</div>
</div>